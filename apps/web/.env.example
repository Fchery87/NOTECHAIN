# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# PWA Configuration
NEXT_PUBLIC_PWA_NAME=NoteChain
NEXT_PUBLIC_PWA_SHORT_NAME=NoteChain
NEXT_PUBLIC_PWA_THEME_COLOR=#1a1a1a
NEXT_PUBLIC_PWA_ICON=./icon.png

# Service Worker
NEXT_PUBLIC_SW_UPDATE_INTERVAL=3600000

# Web Push (optional for notifications)
VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key
VAPID_SUBJECT=mailto:admin@notechain.tech

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_PROJECT_ID=your-project-id

# Encryption Configuration
ENCRYPTION_ALGORITHM=AES-256-GCM
KEY_DERIVATION_ITERATIONS=310000
NONCE_LENGTH=12
AUTH_TAG_LENGTH=16

# Biometrics
BIOMETRIC_UNLOCK_ENABLED=true
BIOMETRIC_RETRY_COUNT=3

# Sync Configuration
SYNC_QUEUE_MAX_SIZE=1000
SYNC_RETRY_DELAY_MS=5000
SYNC_MAX_RETRIES=5

# Analytics (Privacy-Preserving)
ANALYTICS_ENABLED=false
ANALYTICS_ENDPOINT=https://analytics.notechain.tech

# Development
DEBUG_MODE=false
LOG_LEVEL=warn
